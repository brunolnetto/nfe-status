# NFE Status Monitor Configuration
# Copy this file to .env and modify the values as needed

# Target URL (default: official NFe availability page)
NFE_URL=https://www.nfe.fazenda.gov.br/portal/disponibilidade.aspx

# Database configuration
NFE_DB_PATH=data/disponibilidade.db

# JSON output configuration
NFE_JSON_PATH=data/disponibilidade.json

# Logging configuration
NFE_LOG_LEVEL=INFO
NFE_LOG_FILE=logs/nfe_status.log 